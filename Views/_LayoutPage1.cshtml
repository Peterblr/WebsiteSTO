<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="http://fonts.googleapis.com/css?family=Julius+Sans+One"
        rel="stylesheet"
        type="text/css" />
    <link href="~/css/camera.css" rel="stylesheet" />
    <link href="~/css/jquery.lightbox.css" rel="stylesheet" />
    <link href="~/css/style.css" rel="stylesheet" />
    <script src="~/js/camera.min.js"></script>
    <script src="~/js/jquery.easing.1.3.js"></script>
    <script src="~/js/jquery.lightbox.js"></script>
    <script src="~/js/jquery.min.js"></script>
    <script src="~/js/jquery.mobile.customized.min.js"></script>
</head>
<body>
    <div class="h_bg">
        <div class="wrap">
            <div class="wrapper">
                <div class="header">
                    <div class="logo">
                        <a href="#">
                            @*<img src="~/images/logo.png" />*@ 
                            <h1>Training Project</h1> 
                        </a>
                    </div>
                    <div class="cssmenu">
                        <ul>
                            <li>@Html.ActionLink("Home", "Index", "Home", new { area = "" }, new { })</li>
                            <li>@Html.ActionLink("About Us", "About", "Home", new { area = "" }, new { })</li>

                            <li class="has-sub">
                            <li>
                                @Html.ActionLink("Services", "Service", "Home", new { area = "" }, new { })
                                <ul>
                                    <li class="has-sub">
                                    <li>
                                        @Html.ActionLink("Service 1", "Service", "Home", new { area = "" }, new { })
                                        <ul>
                                            <li>@Html.ActionLink("Sub Item 1", "Service", "Home", new { area = "" }, new { })</li>
                                            <li>@Html.ActionLink("Sub Item 2", "Service", "Home", new { area = "" }, new { })</li>
                                        </ul>
                                    </li>
                            </li>
                            <li class="has-sub">
                            <li>
                                @Html.ActionLink("Service 2", "Service", "Home", new { area = "" }, new { })
                                <ul>
                                    <li>@Html.ActionLink("Sub Item 1", "Service", "Home", new { area = "" }, new { })</li>
                                    <li>@Html.ActionLink("Sub Item 2", "Service", "Home", new { area = "" }, new { })</li>
                                </ul>
                            </li>
                            </li>
                        </ul>
                        </li>
                        </li>


                        <li class="active last">
                        <li>@Html.ActionLink("Contact", "Contact", "Home", new { area = "" }, new { })</li>
                        </li>
                        <div class="clear"></div>
                        </ul>
                    </div>
                    <div class="clear"></div>
                </div>
            </div>
        </div>
    </div>
    <div>
        @RenderBody()
    </div>
    <div class="footer_bg">
        <div class="wrap">
            <div class="wrapper">
                <div class="footer">
                    <div class="copy">
                        <p class="w3-link">
                            © All Rights Reserved | Design by&nbsp;
                            <a href="http://google.com/"> Training Project</a>
                        </p>
                    </div>
                    <div class="f_nav">
                        <ul>
                            <li><a href="#">Skype</a></li>
                            <li><a href="#">Linked in</a></li>
                            <li><a href="#">Twitter</a></li>
                            <li><a href="#">Facebook</a></li>
                        </ul>
                    </div>
                    <div class="clear"></div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>
